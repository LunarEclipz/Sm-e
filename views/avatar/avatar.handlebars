		{{#if check_user}}
		<button type='button' class='btn btn-lg btn-info btn-block btn-dark' data-toggle='modal' data-target='#myModal'>
			Delete Avatar
		</button> <br>{{/if}}

	</div>


	<div class="container">

            <div class="modal fade" id="myModal{{id}}" role="dialog">
                <div class="modal-dialog">

                    <!-- Modal content-->
                    <div class="modal-content">
                        <div class="modal-header">
                            <h4 class="modal-title">{{Name}}, Are you sure you want to delete your Avatar & Pet?</h4>
                
                            <button type="button" class="close" data-dismiss="modal">&times;</button>

                        </div>
                        <div class="modal-body">
                            <h5>All chnages cannot be undone. You will lose all your progress and data, are you sure you want to delete?</h5>
                        <div class="modal-body">
                           
                        <div class="modal-footer">
                            <form action="/avatar/deleteAvatar/{{check_user.username}}" method="POST">
                                <button type="submit" class="btn btn-warning"> Delete Pet</button>
                            </form>
                            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>
